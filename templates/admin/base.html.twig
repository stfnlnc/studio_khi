<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Studio Khi{% endblock %}</title>
        <link rel="icon" href="{{ asset('build/images/logos/favicon.svg') }}">
        <link rel="apple-touch-icon" href="{{ asset('build/images/logos/apple-icon.png') }}">
        <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('build/images/logos/apple-icon-ipad.png') }}">
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('build/images/logos/apple-icon-iphone-retina.png') }}">
        <link rel="apple-touch-icon" sizes="167x167" href="{{ asset('build/images/logos/apple-icon-ipad-retina.png') }}">
        <meta name="robots" content="noindex">
        <meta name="description" content="">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta name="robots" content="noindex">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
    {% include 'admin/header/header.html.twig' %}
    <main>
        <div class="header__gap"></div>
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="pointer alert alert__{{ label }} box-shadow">{{ message }}</div>
            {% endfor %}
        {% endfor %}

        {% block body %}{% endblock %}
    </main>
    {% include 'footer/footer.html.twig' %}
    </body>
</html>
