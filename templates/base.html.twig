<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Studio Khi{% endblock %}</title>
        <link rel="icon" href="{{ asset('build/images/logos/favicon.svg') }}">
        <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('build/images/logos/apple-icon.svg') }}">
        <meta name="robots" content="noindex">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        {% block meta %}
            <meta name="description" content="Studio Khi est un studio indépendant qui conçoit des sites et des designs sur mesure pour des marques et des entrepreneurs qui souhaitent se démarquer sur le web.">
            <meta http-equiv="X-UA-Compatible" content="ie=edge">
            <meta property="og:locale" content="fr_FR" />
            <meta property="og:title" content="Studio Khi" />
            <meta property="og:type" content="website" />
            <meta property="og:description" content="Studio Khi est un studio indépendant qui conçoit des sites et des designs sur mesure pour des marques et des entrepreneurs qui souhaitent se démarquer sur le web." />
            <meta property="og:url" content="https://studiokhi.com" />
            <meta property="og:image" content="{{ asset('build/images/logos/apple-icon.svg') }}" />
            <meta property="og:image:secure_url" content="{{ asset('build/images/logos/apple-icon.svg') }}" />
            <meta property="og:image:width" content="152" />
            <meta property="og:image:height" content="152" />
            <meta property="og:site_name" content="Studio Khi" />
        {% endblock %}

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="pointer alert alert__{{ label }}">
                <a class="btn__delete">
                    <button>x</button>
                </a>
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
